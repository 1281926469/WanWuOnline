.content {
  margin-top: 256rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.content .ring image {
  width: 120rpx;
  height: 146rpx;
  vertical-align: middle;
}
.content .hint {
  margin-top: 40rpx;
}
.content .hint text {
  font-family: PingFangSC-Regular;
  font-size: 16px;
  color: #22262D;
  letter-spacing: -0.39px;
  text-align: center;
}
.content button {
  width: 650rpx;
  background: #4AC46D;
  border-radius: 5px;
  color: white;
  height: 88rpx;
  margin: 0;
  padding: 0;
  margin-top: 70rpx;
  line-height: 88rpx;
}
.content button image {
  width: 52rpx;
  height: 45rpx;
  margin-right: 20rpx;
  vertical-align: middle;
}
.content button text {
  font-family: PingFangSC-Regular;
  font-size: 16px;
  color: #FFFFFF;
  letter-spacing: 0;
  padding: 0;
  margin: 0;
}
